        <icon name="menubar22.png" size="22" />
        <!-- Sidebar icon -->
        <icon name="sidebar16.png" size="16" />
        <icon name="sidebar18.png" size="18" />
        <icon name="sidebar32.png" size="32" />
        <!-- Toolbar icon -->
        <icon name="toolbar32.png" size="32" />
        <icon name="toolbar24.png" size="24" />
        <!-- Toolbar icon stramlined -->
        <icon name="toolbar19.png" size="19" />
        <!-- Safari extension icons -->
        <icon name="safari16.png"    size="16" />
        <icon name="safari32.png"    size="32" />
        <icon name="safari48.png"    size="48" />
    </osx>
    
    <windows>
        <!-- App icon -->
        <icon name="application.ico"  size="16,24,32,48,64,128,256"  />
        <!-- Alternate App icon (AIR) -->
        <icon name="appicon16.png"   size="16"  />
        <icon name="appicon24.png"   size="24"  />
        <icon name="appicon32.png"   size="32"  />
        <icon name="appicon48.png"   size="48"  />
        <icon name="appicon64.png"   size="64"  />
        <icon name="appicon128.png"  size="128" />
        <icon name="appicon256.png"  size="256" />

        <!-- Toolbar icon -->
        <icon name="toolbar16.png"   size="16"  />
        <icon name="toolbar24.png"   size="24"  />
        <icon name="toolbar32.png"   size="32"  />
        <!-- Dialog/wizard icons -->
        <icon name="dialog32.png"   size="32"  />
        <icon name="dialog48.png"   size="48"  />
        <!-- Quick launch icon -->
        <icon name="quicklaunch40.png"   size="40"  />
        <icon name="quicklaunch48.png"   size="48"  />
        <!-- Balloon icon -->
        <icon name="balloon32.png"   size="32"  />
        <icon name="balloon48.png"   size="48"  />
    </windows>
    
</Iconology>
